#network-config
network:
  version: 2
  ethernets:
    enp1s0:
      match:
        macaddress: "{{ guest_macaddr }}"
      addresses:
        - {{ guest_ipaddr }}
      routes:
        - to: 0.0.0.0/0
          via: {{ guest_gateway }}
      nameservers:
        addresses:
          {{ guest_resolvers | to_nice_yaml | indent(width=10, first=False) }}
