#network-config
network:
  version: 2
  ethernets:
    enp1s0:
      match:
        macaddress: "{{ guest_macaddr }}"
      addresses:
        - {{ guest_ipaddr }}
      routes:
        - to: 0.0.0.0/0
          via: {{ guest_gateway }}
      nameservers:
        addresses:
          {% for _server in guest_resolvers -%}
          - {{ _server }}
          {%- endfor %}
